<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.developer.postservice.prefix.query.mapper.PrefixMapper">
    <select id="findByUserCode"
            parameterType="Long"
            resultType="com.developer.postservice.prefix.query.dto.MapperPrefixDTO">
        SELECT
            job_tag_code,
            dbti_code
        FROM prefix
        WHERE user_code = #{ userCode }
    </select>

</mapper>